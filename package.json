{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "env-cmd": "^10.1.0",
    "firebase": "^8.3.2",
    "firebase-functions": "^4.4.1",
    "quill": "^2.0.2",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.4",
    "react-dom": "^18.3.1",
    "react-quilljs": "^2.0.4",
    "react-router-bootstrap": "^0.26.3",
    "react-router-dom": "^6.26.1",
    "react-scripts": "^5.0.1",
    "uuid": "^10.0.0",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:dev": "env-cmd -f ./src/utils/.env.dev npm start",
    "start:prod": "env-cmd -f ./src/utils/.env.prod npm start",
    "quickBuild": "env-cmd -f ./src/utils/.env.dev react-scripts build && firebase deploy --only hosting --project=dev",
    "build:dev": "npm run test:dev && env-cmd -f ./src/utils/.env.dev react-scripts build && firebase deploy --only hosting --project=dev",
    "build:prod": "env-cmd -f ./src/utils/.env.prod node generate-buildno.js && react-scripts build && firebase deploy --only hosting --project=prod",
    "deploy-functions:dev": "firebase use dev && firebase deploy --only functions --project=dev",
    "deploy-functions:prod": "firebase use prod && firebase deploy --only functions --project=prod",
    "test": "react-scripts test",
    "cy:run": "npx cypress run",
    "cy:open": "npx cypress open",
    "test:dev": "run-p --race start:dev cy:run",
    "test:prod": "run-p --race start:prod cy:run",
    "test:open": "run-p start:dev cy:open",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "babel-preset-react": "^6.24.1",
    "cypress": "^13.13.3",
    "npm-run-all": "^4.1.5"
  }
}
